#
# English (caps->ctrl, Esc->iWnn on)

type OVERLAY

map key 58 CTRL_LEFT
map key 1 GRAVE
map key 41 YEN

map key 29 BACK
map key 125 HOME
map key 100 MENU

### Basic QWERTY keys ###

key A {
    label:                              'A'
    base:                               'a'
    shift:                              'A'
}

key B {
    label:                              'B'
    base:                               'b'
    shift:                              'B'
}

key C {
    label:                              'C'
    base:                               'c'
    shift:                              'C'
}

key D {
    label:                              'D'
    base:                               'd'
    shift:                              'D'
}

key E {
    label:                              'E'
    base:                               'e'
    shift:                              'E'
}

key F {
    label:                              'F'
